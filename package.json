{
	"name": "@extws/client",
	"version": "0.1.0-beta.5",
	"description": "Client for ExtWS",
	"publishConfig": {
		"access": "public",
		"tag": "beta"
	},
	"type": "module",
	"main": "src/main.js",
	"exports": {
		".": {
			"import": {
				"browser": "./dist/extws-client.js",
				"default": "./src/main.js"
			}
		},
		"./browser": "./dist/extws-client.js"
	},
	"dependencies": {
		"@extws/server": "0.1.0-beta.8",
		"@kirick/event-emitter": "3.0.0",
		"ws": "8.14.2"
	},
	"devDependencies": {
		"@babel/eslint-parser": "7.21.8",
		"eslint": "8.41.0",
		"eslint-config-xo": "0.43.1",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-unicorn": "47.0.0"
	},
	"scripts": {
		"test-node": "VERBOSE=1 NODE_TLS_REJECT_UNAUTHORIZED=0 node test/main.js",
		"test-bun": "VERBOSE=1 NODE_TLS_REJECT_UNAUTHORIZED=0 bun run test/main.js",
		"build": "bun run build-full && bun run build-minified",
		"build-full": "OUT=extws-client.js bun run make/bun.build.js",
		"build-minified": "OUT=extws-client.min.js MINIFY=1 bun run make/bun.build.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/extws-team/client.git"
	},
	"keywords": [
		"extws",
		"websocket",
		"client",
		"rtc"
	],
	"author": "Daniil Kirichenko (https://twitter.com/kirickme)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/extws-team/client/issues"
	},
	"homepage": "https://github.com/extws-team/client#readme"
}
